<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%- title %> | CSE 340 App</title>
    
    <!-- Favicon -->
    <link rel="icon" type="image/png" sizes="32x32" href="/images/site/favicon-32x32.png">
    <link rel="apple-touch-icon" href="/images/site/icon.png">
    
    <!-- SEO Meta Tags -->
    <meta name="description" content="CSE Motors - Your premier destination for quality vehicles. Browse our inventory of cars, trucks, and SUVs.">
    <meta name="keywords" content="cars, vehicles, auto sales, CSE Motors, used cars, automotive">
    <meta name="author" content="CSE Motors">
    
    <!-- Open Graph / Facebook / LinkedIn -->
    <meta property="og:type" content="website">
    <meta property="og:url" content="https://csemotors.com/">
    <meta property="og:title" content="<%- title %> | CSE Motors">
    <meta property="og:description" content="CSE Motors - Your premier destination for quality vehicles. Browse our inventory of cars, trucks, and SUVs.">
    <meta property="og:image" content="https://csemotors.com/images/vehicles/delorean.jpg">
    <meta property="og:site_name" content="CSE Motors">
    <meta property="og:locale" content="en_US">
    
    <!-- Twitter Card -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:url" content="https://csemotors.com/">
    <meta name="twitter:title" content="<%- title %> | CSE Motors">
    <meta name="twitter:description" content="CSE Motors - Your premier destination for quality vehicles. Browse our inventory of cars, trucks, and SUVs.">
    <meta name="twitter:image" content="https://csemotors.com/images/vehicles/delorean.jpg">
    <meta name="twitter:creator" content="@csemotors">
    <meta name="twitter:site" content="@csemotors">
    
    <!-- Snapchat Pixel (replace with actual Pixel ID) -->
    <meta name="snapchat-pixel-id" content="YOUR_SNAPCHAT_PIXEL_ID">
    
    <!-- LinkedIn Insights Tag (if you have one) -->
    <meta name="linkedin-insights" content="YOUR_LINKEDIN_PARTNER_ID">
    
    <!-- Pinterest -->
    <meta name="pinterest-rich-pin" content="true">
    <meta property="og:image:width" content="1200">
    <meta property="og:image:height" content="630">
    
    <!-- WhatsApp (uses Open Graph) but you can add image alt -->
    <meta property="og:image:alt" content="Silver DMC DeLorean sports car - CSE Motors featured vehicle">
    
    <!-- Microsoft Tiles (Windows) -->
    <meta name="msapplication-TileColor" content="#143a6c">
    <meta name="msapplication-TileImage" content="/images/site/icon.png">
    
    <!-- Theme Color (for mobile browsers) -->
    <meta name="theme-color" content="#143a6c">
    <meta name="apple-mobile-web-app-status-bar-style" content="#143a6c">
    
    <!-- Canonical URL (prevents duplicate content) -->
    <link rel="canonical" href="https://csemotors.com/">
    
    <!-- Robots -->
    <meta name="robots" content="index, follow, max-image-preview:large, max-snippet:-1, max-video-preview:-1">
    
    <!-- Additional Open Graph for better sharing -->
    <meta property="og:image:type" content="image/jpeg">
    <meta property="fb:app_id" content="YOUR_FACEBOOK_APP_ID">
    
    <!-- Schema.org Structured Data for Rich Snippets -->
    <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "AutoDealer",
      "name": "CSE Motors",
      "description": "Your premier destination for quality vehicles",
      "url": "https://csemotors.com",
      "logo": "https://csemotors.com/images/site/icon.png",
      "image": "https://csemotors.com/images/vehicles/delorean.jpg",
      "priceRange": "$$",
      "address": {
        "@type": "PostalAddress",
        "addressCountry": "US"
      },
      "sameAs": [
        "https://twitter.com/csemotors",
        "https://facebook.com/csemotors",
        "https://linkedin.com/company/csemotors"
      ]
    }
    </script>
    
    <link rel="stylesheet" href="/css/styles-mobile.css">
    <link rel="stylesheet" href="/css/styles-tablet.css" media="screen and (min-width: 600px) and (max-width: 1023px)">
    <link rel="stylesheet" href="/css/styles-laptop.css" media="screen and (min-width: 1024px) and (max-width: 1440px)">
    <link rel="stylesheet" href="/css/styles-large.css" media="screen and (min-width: 1441px)">
</head>